SRC =	tests.cpp		\
		Account.class.cpp
NAME = account
FLAGS = -Wall -Wextra -Werror

all : $(NAME)

$(NAME):
	g++ -o $(NAME) $(FLAGS) $(SRC)

fclean:
	rm $(NAME)

re: fclean all
